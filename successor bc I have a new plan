    def getSuccessor(self, suit, rank):
        card = Card(suit, rank)
        succ = None
        if not self.root:
            return None
        for i in self.inOrderer():
            if i > card:
                if succ is None:
                    succ = i
                else:
                    if i < succ:
                        succ = i
                    else:
                        continue
        return succ
